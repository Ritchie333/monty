 $CD2C ADD HL,HL     ;
 $CD2D ADD HL,HL     ;
 $CD2E ADD HL,HL     ; }
 $CD2F EX DE,HL      ; Store it in #REGde.
 $CD30 POP HL        ; { Restore #REGhl and return.
 $CD31 RET           ; }

