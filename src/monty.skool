 $EC39 DEC HL        ;
 $EC3A DEC HL        ;
 $EC3B LD ($ECE2),HL ; }
 $EC3E POP BC        ; { Loop back if there are more cells to copy.
 $EC3F DJNZ $EC1A    ; }
 $EC41 LD A,($ECDF)  ; { Point #REGhl at the next pointer to the screen address table.
 $EC44 AND $F8       ;
 $EC46 SRL A         ;
 $EC48 SRL A         ;
 $EC4A SRL A         ;
